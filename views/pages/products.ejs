<% include ../partials/header %>
 <table class="table">
     <thead>
         <tr>
             <th>ID</th>
             <th>Title</th>
             <th>Price</th>
             <th>Edit</th>
             <th><a name="" id="" class="btn btn-success" href="/insert" role="button">Add New</a></td></th>
         </tr>
     </thead>
     <tbody>
     <% products.forEach(function(product){ %>
         <tr>
             <td scope="row"><%= product.id %></td>
             <td><%= product.title %></td>
             <td><%= product.price %></td>
             <td><a name="" id="" class="btn btn-dark" href="/products/<%= product.id %>" role="button">Edit</a>
             <a name="" id="" class="btn btn-danger" href="" role="button">Delete</a></td>
         </tr>
         </tr>
         <% }); %>
             
     </tbody>
 </table>




<% include ../partials/footer %>